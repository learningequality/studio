    "education",
    "content",
    "learning",
    "edtech"
  ],
  "author": "Learning Equality",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/learningequality/studio/issues"
  },
  "homepage": "https://github.com/learningequality/studio#readme",
