<div class="input-tab-control tab_item" data-next=".toggle_id"></div>
<div class="modal-header">
  <button type="button" class="close tab_item" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
  <h4 class="modal-title">
    {{#if published}}{{formatMessage (intlGet 'messages.published')}}
    {{else}}{{#if get_id}}{{formatMessage (intlGet 'messages.channel_publish_id')}}
    {{else}}{{formatMessage (intlGet 'messages.channel_publish_token')}}{{/if}}{{/if}}
  </h4>
</div>
<div class="modal-body">
  <div class="modal-body-content published_wrapper">
    <p>{{#if get_id}}{{formatMessage (intlGet 'messages.published_prompt')}}{{else}}{{formatMessage (intlGet 'messages.published_token_prompt')}}{{/if}}</p>
    <h4 class="text-center publish_id_wrapper">
      <input type="text" id="modal-copy-text" class="publish-id-text" value="{{#if get_id}}{{ channel.id }}{{else}}{{channel.primary_token}}{{/if}}" readonly/>
      <button class="btn action-button copy-id-btn tab_item first_focus_item uppercase" id="modal-copy-btn">{{formatMessage (intlGet 'messages.copy')}}</button>
    </h4>
    <br/>
    <br/>
    <p>
    {{#if get_id}}{{formatMessage (intlGet 'messages.newer_version')}}{{else}}{{formatMessage (intlGet 'messages.older_version')}}{{/if}}
    <button class="btn action-text tab_item toggle_id">{{#if get_id}}{{formatMessage (intlGet 'messages.get_token')}}{{else}}{{formatMessage (intlGet 'messages.get_id')}}{{/if}}</button>
    </p>
  </div>
</div>
<div class="input-tab-control tab_item" data-next=".close"></div>